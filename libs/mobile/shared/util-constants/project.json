{
  "name": "mobile-shared-util-constants",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/mobile/shared/util-constants/src",
  "prefix": "smart-home",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/mobile/shared/util-constants/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/mobile/shared/util-constants/**/*.ts",
          "libs/mobile/shared/util-constants/**/*.html"
        ]
      }
    }
  },
  "tags": [
    "platform:mobile",
    "scope:shared",
    "type:util"
  ]
}
